<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	  <welcome-file-list>
	    <welcome-file>welcome.jsp</welcome-file>
	  </welcome-file-list>
	<listener>
	  <listener-class>com.cyb.kiiik.SSOSessionListener</listener-class>
    </listener>
	<filter>
	    <filter-name>SSOAUTOLOGIN</filter-name>
	    <filter-class>com.kiiik.sso.AutoLogin</filter-class>
	    <init-param>
	      <param-name>casServer</param-name>
	      <param-value>http://auth.cyb.com/casserver/</param-value>
	    </init-param>
	    <init-param>
	      <param-name>serverName</param-name>
	      <param-value>http://first.cyb.com/casclient1/</param-value>
	    </init-param>
   </filter>
   <filter-mapping>
    <filter-name>SSOAUTOLOGIN</filter-name>
    <url-pattern>/*</url-pattern>
  </filter-mapping>
 <servlet> 
	<servlet-name>logout</servlet-name> 
	<servlet-class>com.cyb.kiiik.LoginOutServlet</servlet-class> 
 </servlet> 
<servlet-mapping> 
	<servlet-name>logout</servlet-name> 
	<url-pattern>/logout</url-pattern> 
</servlet-mapping> 
</web-app>